#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xeba430 .scope module, "pwmtopcmtest" "pwmtopcmtest" 2 3;
 .timescale -9 -12;
v0xed0390_0 .net "HyperMegaPCM", 0 0, v0xea85e0_0; 1 drivers
v0xed0430_0 .var "SuperMegaX4", 0 0;
v0xed04e0_0 .var "clk", 0 0;
v0xed0590_0 .var "clk3", 0 0;
v0xed0670_0 .var "clk8", 0 0;
S_0xeba520 .scope module, "uut" "pwm2pcm" 2 15, 3 16, S_0xeba430;
 .timescale 0 0;
v0xea85e0_0 .var "HyperMegaPCM", 0 0;
v0xecfe90_0 .var "PCMCounter", 1 0;
v0xecff30_0 .net "SuperMegaX4", 0 0, v0xed0430_0; 1 drivers
v0xecffd0_0 .net "clk", 0 0, v0xed04e0_0; 1 drivers
v0xed0080_0 .net "clk3", 0 0, v0xed0590_0; 1 drivers
v0xed0120_0 .net "clk8", 0 0, v0xed0670_0; 1 drivers
v0xed0200_0 .var "currentPWMCounter", 2 0;
v0xed02a0_0 .var "oldPWMCounter", 2 0;
E_0xea73d0 .event edge, v0xea85e0_0;
E_0xeba610 .event negedge, v0xed0080_0;
E_0xeba6f0 .event posedge, v0xed0120_0;
E_0xeba820 .event posedge, v0xecffd0_0;
    .scope S_0xeba520;
T_0 ;
    %wait E_0xeba820;
    %load/v 8, v0xed0200_0, 3;
    %ix/load 0, 3, 0;
    %assign/v0 v0xed02a0_0, 0, 8;
    %ix/load 0, 3, 0;
    %assign/v0 v0xed0200_0, 0, 0;
    %ix/load 0, 2, 0;
    %assign/v0 v0xecfe90_0, 0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0xeba520;
T_1 ;
    %wait E_0xeba6f0;
    %load/v 8, v0xecff30_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_1.0, 4;
    %load/v 8, v0xed0200_0, 3;
    %mov 11, 0, 29;
    %addi 8, 1, 32;
    %ix/load 0, 3, 0;
    %assign/v0 v0xed0200_0, 0, 8;
T_1.0 ;
    %load/v 8, v0xecff30_0, 1;
    %mov 9, 0, 1;
    %cmpi/u 8, 0, 2;
    %jmp/0xz  T_1.2, 4;
    %ix/load 0, 3, 0;
    %assign/v0 v0xed0200_0, 0, 0;
T_1.2 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0xeba520;
T_2 ;
    %wait E_0xeba610;
    %load/v 8, v0xecfe90_0, 2;
    %mov 10, 0, 1;
    %cmpi/u 8, 0, 3;
    %jmp/0xz  T_2.0, 4;
    %load/v 8, v0xed02a0_0, 1; Only need 1 of 3 bits
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %assign/v0 v0xea85e0_0, 0, 8;
    %load/v 8, v0xecfe90_0, 2;
    %mov 10, 0, 30;
    %addi 8, 1, 32;
    %ix/load 0, 2, 0;
    %assign/v0 v0xecfe90_0, 0, 8;
T_2.0 ;
    %load/v 8, v0xecfe90_0, 2;
    %mov 10, 0, 1;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_2.2, 4;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.4, 4;
    %load/x1p 8, v0xed02a0_0, 1;
    %jmp T_2.5;
T_2.4 ;
    %mov 8, 2, 1;
T_2.5 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %assign/v0 v0xea85e0_0, 0, 8;
    %load/v 8, v0xecfe90_0, 2;
    %mov 10, 0, 30;
    %addi 8, 1, 32;
    %ix/load 0, 2, 0;
    %assign/v0 v0xecfe90_0, 0, 8;
T_2.2 ;
    %load/v 8, v0xecfe90_0, 2;
    %mov 10, 0, 2;
    %cmpi/u 8, 2, 4;
    %jmp/0xz  T_2.6, 4;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.8, 4;
    %load/x1p 8, v0xed02a0_0, 1;
    %jmp T_2.9;
T_2.8 ;
    %mov 8, 2, 1;
T_2.9 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %assign/v0 v0xea85e0_0, 0, 8;
    %ix/load 0, 2, 0;
    %assign/v0 v0xecfe90_0, 0, 0;
T_2.6 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0xeba520;
T_3 ;
    %wait E_0xea73d0;
    %load/v 8, v0xea85e0_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %jmp/0xz  T_3.0, 4;
    %delay 3567587328, 232;
    %ix/load 0, 1, 0;
    %assign/v0 v0xea85e0_0, 0, 0;
T_3.0 ;
    %jmp T_3;
    .thread T_3, $push;
    .scope S_0xeba430;
T_4 ;
    %set/v v0xed0430_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0xeba430;
T_5 ;
    %set/v v0xed04e0_0, 0, 1;
T_5.0 ;
    %delay 12000, 0;
    %load/v 8, v0xed04e0_0, 1;
    %inv 8, 1;
    %set/v v0xed04e0_0, 8, 1;
    %jmp T_5.0;
    %end;
    .thread T_5;
    .scope S_0xeba430;
T_6 ;
    %set/v v0xed0590_0, 0, 1;
T_6.0 ;
    %delay 4000, 0;
    %load/v 8, v0xed0590_0, 1;
    %inv 8, 1;
    %set/v v0xed0590_0, 8, 1;
    %jmp T_6.0;
    %end;
    .thread T_6;
    .scope S_0xeba430;
T_7 ;
    %set/v v0xed0670_0, 1, 1;
T_7.0 ;
    %delay 1500, 0;
    %load/v 8, v0xed0670_0, 1;
    %inv 8, 1;
    %set/v v0xed0670_0, 8, 1;
    %jmp T_7.0;
    %end;
    .thread T_7;
    .scope S_0xeba430;
T_8 ;
    %vpi_call 2 44 "$monitor", "[%g | clk = %b, clk8 = %b, clk3 = %b] PCM output = %d", $time, v0xed04e0_0, v0xed0590_0, v0xed0670_0, v0xed0390_0;
    %delay 12000, 0;
    %set/v v0xed0430_0, 1, 1;
    %delay 6000, 0;
    %set/v v0xed0430_0, 0, 1;
    %delay 18000, 0;
    %set/v v0xed0430_0, 1, 1;
    %delay 12000, 0;
    %set/v v0xed0430_0, 0, 1;
    %delay 12000, 0;
    %set/v v0xed0430_0, 1, 1;
    %delay 18000, 0;
    %set/v v0xed0430_0, 0, 1;
    %delay 30000, 0;
    %vpi_call 2 52 "$finish";
    %end;
    .thread T_8;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "pwm2pcm_tb.v";
    "pwm2pcm.v";
